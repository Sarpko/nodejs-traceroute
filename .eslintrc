{
  "parser": "babel-eslint",
  "extends": [
    "airbnb-base"
  ],
  "env": {
    "node": true,
    "es6": true,
    "mocha": true
  },
  "plugins": [

  ],
  "rules": {
    "no-console": "off",
    "import/prefer-default-export": "off",
    "class-methods-use-this": "off",
    "max-len": [2, 120]
  },
  "parserOptions": {
    "sourceType": "module"
  }
}